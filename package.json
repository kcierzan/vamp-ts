{
  "name": "vamp-ts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev --host 127.0.0.1",
    "build": "vite build",
    "preview": "vite preview",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check src/ tests/ && eslint src/ tests/",
    "format": "prettier --write .",
    "test:integration": "npx playwright install && playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/audioworklet": "^0.0.54",
    "@types/eslint": "8.56.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.7.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.36.0-next.7",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "5.0.0-next.80",
    "svelte-check": "^3.6.6",
    "svelte-dnd-action": "^0.9.40",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.7",
    "fft.js": "^4.0.4",
    "lodash": "^4.17.21",
    "svelte-file-dropzone": "^2.0.4",
    "tone": "^14.8.49",
    "wavesurfer.js": "^7.7.3",
    "web-audio-beat-detector": "^8.2.4"
  }
}
